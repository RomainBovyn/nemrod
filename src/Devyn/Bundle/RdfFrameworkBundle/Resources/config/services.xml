<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="rdf_namespace_registry.class">Devyn\Bridge\EasyRdf\NamespaceRegistry</parameter>
        <parameter key="rdf_mapping_driver.class">Devyn\Component\TypeMapper\Driver\NamespaceRegistry</parameter>
    </parameters>

    <services>
        <service id="rdf_namespace_registry" class="%rdf_namespace_registry.class%">
        </service>
        <service id="rdf_mapping_driver" class="%rdf_mapping_driver.class%">
        </service>

    </services>
</container>
